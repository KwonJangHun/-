   ],
   "source": [
    "array = [7,6,4,9,8,5,3,1,2]\n",
    "\n",
    "for i in range(len(array)):\n",
    "    min_index = i\n",
    "\n",
    "    for l in range(min_index+1, len(array)):\n",
    "        if array[min_index] > array[l]:\n",
    "            min_index = l\n",
    "    array[min_index], array[i] = array[i], array[min_index]\n",
    "print(array)\n",
    "                \n"
   ]
